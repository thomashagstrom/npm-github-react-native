{
  "name": "@thomashagstrom/npm-github-react-native",
  "author": "Thomas HagstroÌˆm",
  "version": "0.1.4",
  "license": "UNLICENSED",
  "publishConfig": {
    "@thomashagstrom:registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomashagstrom/npm-github-react-native.git"
  },
  "bugs": {
    "url": "https://github.com/thomashagstrom/npm-github-react-native/issues"
  },
  "homepage": "https://github.com/thomashagstrom/npm-github-react-native#readme",
  "keywords": [
    "react-native",
    "expo"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "android": "expo start --android",
    "build:watch": "tsc -p ./tsconfig.json --watch",
    "build": "tsc -p ./tsconfig.json",
    "contr:add": "all-contributors add",
    "contr:generate": "all-contributors generate",
    "ios": "expo start --ios",
    "lint:ci": "tsdx lint",
    "lint": "tsdx lint src --fix --ignore-pattern **/*.test.ts",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build && husky install",
    "preversion": "npm run lint && npm run test",
    "size": "size-limit",
    "start:native": "expo start",
    "start": "npm run build:watch",
    "test:cov": "npm run test --coverage",
    "test:dev": "jest --maxWorkers=25%",
    "test:watch": "jest --watchAll --maxWorkers=25%",
    "test": "jest --forceExit --runInBand --ci",
    "version:next": "npm version patch"
  },
  "peerDependencies": {
    "expo": ">=44.0.0",
    "react": ">=17.0.0",
    "react-native": ">=0.64.0",
    "react-native-paper": ">=4.11.0",
    "react-native-web": ">=0.17.0"
  },
  "module": "dist/npm-github-react-native.esm.js",
  "size-limit": [
    {
      "path": "dist/npm-github-react-native.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/npm-github-react-native.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@expo/webpack-config": "^0.16.7",
    "@react-native-community/masked-view": "^0.1.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^9.0.0",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/moment": "^2.13.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.2",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@types/uuid": "^8.3.1",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "core-js": "^3.19.1",
    "expo": "^44.0.6",
    "expo-status-bar": "^1.2.0",
    "husky": "^7.0.4",
    "jest-expo": "^44.0.1",
    "jest-html-reporter": "^3.4.2",
    "jest-junit": "^13.0.0",
    "jest-junit-reporter": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.64.3",
    "react-native-paper": "^4.11.2",
    "react-native-web": "^0.17.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.70.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "weak-napi": "^2.0.2"
  }
}
